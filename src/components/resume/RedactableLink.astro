---
interface Props {
  prefix: string;
  rest: string;
}

const { prefix, rest } = Astro.props;
const redacted = (original: () => string) =>
  process.env.VERCEL_ENV === "production"
    ? "javascript:alert('This has been redacted for privacy reasons.')"
    : original();
---

<a
  class="ignored-link text-blue hover:underline"
  href={redacted(() => `${prefix}${rest}`)}>{rest}</a
>
